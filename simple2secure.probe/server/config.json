[
	{
		"version": 42, 
		"config_supplier": "http://localhost:8080/api/config",
		"task_supplier": "http://localhost:8080/api/getTasks",
		"stylesheet": "/gui/css/simple2relax.css",
		"use_configured_iface": "false",
		"interface_number": "0",
		"show_interfaces": "false",
		"external_address": "monitor.simple2relax.com",
		"connection_timeout": "1000",
		"processing_factory": "com.simple2relax.probe.processor.DefaultPacketProcessingFactory",
		"communication_factory": "com.simple2relax.probe.comm.LocalCommConfiguration",
		"db_config": {
		  "location": "plocal",
		  "dbURI": "/temp/mydb",
		  "write_user": "admin",
		  "write_password": "admin",
		  "read_user": "report",
		  "read_password": "report",
		  "time_slot_size": "360000"
		},
		"queries": {
		  "querystrings": "src/test/resources/osqueries.json",
		  "queryconfig": "src/test/resources/osqueryconfig.json",
		  "osquerypath": ".\\\\bin\\\\osquery\\\\os_win7"
		},
		"buttons": {
			"size": {
				"height": "64",
				"width": "64"
			},
			"button": [
				{
				  "name": "overview",
				  "gui_class": "com.simple2relax.probe.gui.handler.RedirectHandler:/gui/html/overview.html",
				  "icon": "/gui/images/cmd-24.png",
				  "controller": "com.simple2relax.probe.gui.controller.html.HtmlOverviewController"
				},
				{
				  "name": "osquery",
				  "gui_class": "com.simple2relax.probe.gui.handler.RedirectHandler:OsQueryOverview",
				  "icon": "/gui/images/new-24.png"
				},
				{
				  "name": "about",
				  "gui_class": "com.simple2relax.probe.gui.handler.RedirectHandler:AboutView",
				  "icon": "/gui/images/man-24.png"
				},
				{
				  "name": "settings",
				  "gui_class": "com.simple2relax.probe.gui.handler.ContextMenuHandler",
				  "icon": "/gui/images/settings-24.png",
				  "menu": {
					"menuitem": [
					  {
						"name": "language",
						"gui_class": "com.simple2relax.probe.gui.handler.MenuItemRedirectHandler:LanguageView"
					  },
					  {
						"name": "Setting2",
						"gui_class": "com.simple2relax.probe.gui.handler.MenuItemRedirectHandler:Settings2View"
					  }
					]
				  }
				},
				{
				  "name": "exit",
				  "gui_class": "com.simple2relax.probe.gui.handler.ExitHandler",
				  "icon": "/gui/images/cross-24.png"
				}				
			]			
		}
	}
]