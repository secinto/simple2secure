{
  "$schema": "http://json-schema.org/schema#",
  "title": "JSON Query Run Information",
  "type": "array",
  "items": {
    "title": "Query Run",
    "type": "object",
    "properties": {
        "id": {
            "description": "The unique identifier for a query string",
            "type": "string"
        },
        "querystring": {
            "description": "The string for a query",
            "type": "string"
        },
        "timestamp": {
            "description": "The timestamp for the query run",
            "type": "string"
        },
        "queryresult": {
            "description": "The result of the query",
            "type": "string"
        }
    },
    "required": ["id", "querystring"]
  }
}